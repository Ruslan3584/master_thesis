@inproceedings{lopatka_stop_cond,
author = {Schlesinger, Michail and Vodolazskiy, Evgeniy and Lopatka, Nikolai},
title = {Stop Condition for Subgradient Minimization in Dual Relaxed (Max,+) Problem},
year = {2011},
isbn = {9783642230936},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
abstract = {Subgradient descent methods for minimization of dual linear relaxed labeling problem are analysed. They are guaranteed to converge to the quality of the optimal relaxed labeling, but do not obtain an optimal relaxed labeling itself. Moreover, no stop condition is known for these methods upto now. The stop condition is defined and experimentally compared with the commonly-used stop conditions. The stop condition is defined in a way that when fulfilled a relaxed labeling is simultaneously obtained with arbitrary non-zero difference from the optimal labeling.},
booktitle = {Proceedings of the 8th International Conference on Energy Minimization Methods in Computer Vision and Pattern Recognition},
pages = {118–131},
numpages = {14},
keywords = {subgradient descent, energy minimization, labeling problem, equivalent transformation, reparametrisation, relaxation},
location = {St. Petersburg, Russia},
series = {EMMCVPR'11}
}

